{
  "name": "fenxiao",
  "version": "1.0.1",
  "author": "adam198824@gmail.com",
  "homepage": "http://xin-lai.com/",
  "private": true,
  "scripts": {
    "start": "npm run dev",
    "dev": "ionic-app-scripts serve -r 35730 --dev-logger-port 53705",
    "clean": "ionic-app-scripts clean",
    "build": "npm run android&&cordova build ios --release",
    "lint": "ionic-app-scripts lint",
    "prod": "cross-env-shell BUILD_ENV=prod \"ionic-app-scripts build --prod --aot&&npm run rmfonts\"",
    "test": "cross-env-shell BUILD_ENV=dev \"ionic-app-scripts build --prod --aot&&npm run rmfonts\"",
    "ionic:build": "ionic-app-scripts build",
    "ionic:serve": "ionic-app-scripts serve",
    "android": "cross-env-shell BUILD_TARGET=android \"npm run prod&&cordova build android --release\"",
    "android:test": "cross-env-shell BUILD_TARGET=android \"npm run test&&cordova build android --release\"",
    "ios": "npm run prod&&cordova build ios",
    "ios:test": "npm run test&&cordova build ios",
    "web": "cross-env-shell BUILD_TARGET=web \"npm run prod",
    "rmfonts": "rimraf ./www/assets/fonts",
    "device": "cross-env-shell BUILD_TARGET=android \"ionic cordova run android -lc --device\""
  },
  "dependencies": {
    "@angular/animations": "5.2.11",
    "@angular/common": "5.2.11",
    "@angular/compiler": "5.2.11",
    "@angular/compiler-cli": "5.2.11",
    "@angular/core": "5.2.11",
    "@angular/forms": "5.2.11",
    "@angular/http": "5.2.11",
    "@angular/platform-browser": "5.2.11",
    "@angular/platform-browser-dynamic": "5.2.11",
    "@ionic-native/app-minimize": "^4.11.0",
    "@ionic-native/core": "^4.11.0",
    "@ionic-native/file": "^4.11.0",
    "@ionic-native/file-opener": "^4.11.0",
    "@ionic-native/file-transfer": "^4.11.0",
    "@ionic-native/image-picker": "^4.11.0",
    "@ionic-native/splash-screen": "^4.11.0",
    "@ionic-native/status-bar": "^4.11.0",
    "@ionic/storage": "2.1.3",
    "cordova-android": "7.0.0",
    "cordova-android-support-gradle-release": "^1.4.5",
    "cordova-plugin-appminimize": "^1.0.0",
    "cordova-plugin-device": "^2.0.2",
    "cordova-plugin-ionic-keyboard": "^2.1.3",
    "cordova-plugin-ionic-webview": "^1.2.1",
    "cordova-plugin-splashscreen": "^5.0.2",
    "cordova-plugin-statusbar": "^2.4.2",
    "cordova-plugin-whitelist": "^1.3.3",
    "ionic-angular": "3.9.2",
    "ionicons": "3.0.0",
    "moment": "^2.22.2",
    "rxjs": "5.5.11",
    "sw-toolbox": "3.6.0",
    "zone.js": "0.8.26"
  },
  "devDependencies": {
    "@ionic/app-scripts": "3.1.10",
    "cross-env": "^5.2.0",
    "nswag": "^11.17.19",
    "typescript": "2.7.2"
  },
  "description": "分销",
  "config": {
    "ionic_webpack": "./config/webpack.config.js"
  },
  "cordova": {
    "plugins": {
      "cordova-plugin-whitelist": {},
      "cordova-plugin-device": {},
      "cordova-plugin-splashscreen": {},
      "cordova-plugin-ionic-webview": {},
      "cordova-plugin-ionic-keyboard": {},
      "cordova-plugin-statusbar": {},
      "cordova-plugin-appminimize": {},
      "cordova-android-support-gradle-release": {
        "ANDROID_SUPPORT_VERSION": "27.+"
      }
    },
    "platforms": []
  }
